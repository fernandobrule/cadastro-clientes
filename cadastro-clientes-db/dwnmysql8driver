#!/bin/bash

echo "################################################################################################"
echo "About to download the Jdbc Driver"
echo ""
curl -O https://repo1.maven.org/maven2/mysql/mysql-connector-java/8.0.22/mysql-connector-java-8.0.22.jar > mysql-connector-java-8.0.22.jar \
     && mv mysql-connector-java-8.0.22.jar lib/
echo "################################################################################################"
pwd
echo "################################################################################################"
ls -l
echo "################################################################################################"
